create database cms;
use cms;

create table year (
       id INT,
       year YEAR(4),
       );

create table season (
       id INT,
       season VARCHAR(100),
       );

create table semester (
       id INT,
       sem_start DATE,
       sem_end DATE
       );

create table course (
       id INT,
       course VARCHAR(100),
       );

